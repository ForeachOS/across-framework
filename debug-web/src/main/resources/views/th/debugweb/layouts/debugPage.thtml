<!DOCTYPE html>
<html xmlns:th="http://www.thymeleaf.org">
<head>
	<title>Debug controller</title>
	<link rel="stylesheet" type="text/css" th:each="css : ${webResourceRegistry.getResources('css')}" th:href="@{${css.data}}"/>
	<script th:each="javascript : ${webResourceRegistry.getResources('javascript')}" th:src="${javascript.data}" th:if="${javascript.location != 'inline'}"></script>
</head>
<body>
<div class="container">
	<div class="row">
		<nav class="navbar navbar-default" role="navigation">
			<div class="collapse navbar-collapse">
				<div class="nav navbar-left">
					<div class="debug-warning">debug</div>
				</div>

				<ul class="nav navbar-nav">
					<li th:each="menu : ${debugMenu.items}" th:class="${menu.hasItems()}? 'dropdown'" th:classappend="${menu.selected}? 'active'">
						<a th:unless="${menu.hasItems()}" th:href="@{${menu.url}}" th:text="${menu.title}">main item</a>
						<a th:if="${menu.hasItems()}" href="#" class="dropdown-toggle" data-toggle="dropdown"><span th:text="${menu.title}"></span>
							<b class="caret"></b></a>
						<ul th:if="${menu.hasItems()}" class="dropdown-menu">
							<div th:each="subItem : ${menu.items}" th:remove="tag">
								<li th:unless="${subItem.isGroup()}" th:classappend="${subItem.selected}? 'active'">
									<a th:href="@{${subItem.url}}" th:text="${subItem.title}">sub item</a>
								</li>
								<div th:if="${subItem.isGroup() and subItem.hasItems()}" th:remove="tag">
									<li th:unless="${subItemStat.first}" class="divider"></li>
									<li class="dropdown-header" th:if="${subItem.hasTitle()}" th:text="${subItem.title}"></li>
									<li th:each="subSubItem : ${subItem.items}" th:classappend="${subSubItem.selected}? 'active'">
										<a th:href="@{${subSubItem.url}}" th:text="${subSubItem.title}">sub item</a>
									</li>
									<li th:unless="${subItemStat.last}" class="divider"></li>
								</div>
							</div>

							<!--/*
							<li th:each="subItem : ${menu.items}" th:classappend="${subItem.selected}? 'active'">
								<a th:href="@{${subItem.url}}" th:text="${subItem.title}">sub item</a>
							</li>
							*/-->
						</ul>
					</li>
				</ul>

			</div>
		</nav>
	</div>

	<div class="row" th:if="${debugMenu.selected}">
		<ol class="breadcrumb">
			<li th:each="item : ${debugMenu.selectedItemPath}" th:classappend="${itemStat.last} ? 'active'" th:if="${item.hasTitle()}">
				<span th:if="${itemStat.last}" th:text="${item.title}">title selected item</span>
				<div th:unless="${itemStat.last}" th:remove="tag">
					<span th:if="${!item.hasUrl() and !item.isGroup()}" th:text="${item.title}">title if no url</span>
					<a th:if="${item.hasUrl() and !itemStat.last}" th:href="${item.url}" th:text="${item.title}">title
						with
						url</a>
					<a th:if="${!item.hasUrl() and item.isGroup() and item.hasItems()}" th:href="${item.firstItem.url}" th:text="${item.title}">title
						with first item url</a>
				</div>
			</li>
		</ol>
	</div>

	<!--
	<ul th:replace="th/bootstrap/menu :: dropdown (${debugMenu},'debug-menu')" th:if="${debugMenu}"></ul>
	-->

	<div th:if="${childPage != '' and childPage != null}">
		<div th:replace="${childPage} :: content">
		</div>
	</div>
</div>

<p>&nbsp;</p>

<script th:each="javascript : ${webResourceRegistry.getResources('javascript-page-end')}" th:src="@{${javascript.data}}"
        th:if="${javascript.location == 'external'}"></script>
</body>
</html>